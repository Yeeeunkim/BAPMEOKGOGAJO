<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings><!-- 기본세팅 설정 -->
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.bob.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.bob.notice.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.bob.notice.model.vo.Attachment" alias="Attachment"/>
		<typeAlias type="com.kh.bob.notice.model.vo.Comment" alias="Comment"/>
		<!-- 테스트용 -->
		<typeAlias type="com.kh.bob.shop.model.vo.Shop" alias="Shop"/>
		<typeAlias type="com.kh.bob.shop.model.vo.Reserve" alias="Reserve"/>
		<typeAlias type="com.kh.bob.shop.model.vo.Menu" alias="Menu"/>
		<typeAlias type="com.kh.bob.shop.model.vo.Table" alias="Table"/>
		
	</typeAliases>
	 
	<mappers><!-- DB쿼리문 담을 mapper파일 등록 태그-->
		<mapper resource="mapper/member-mapper.xml"/>
		<mapper resource="mapper/board-mapper.xml"/>
		<mapper resource="mapper/shop-mapper.xml"/>
	</mappers>
	
</configuration>